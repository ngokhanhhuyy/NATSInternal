<!DOCTYPE html>
<html lang="vi">
    <head>
        <title>@ViewBag.PageTitle - NATSInternal</title>
        <link rel="icon" type="image/x-icon" href="~/output/images/vite.svg">
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link
            href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap"
            rel="stylesheet">
        <link href="~/output/css/style.css" type="text/css" rel="stylesheet" asp-append-version="true"/>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.13.1/font/bootstrap-icons.min.css" rel="stylesheet">
        @await RenderSectionAsync("Styles", required: false)
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

        <script src="~/output/js/class-name-processor.js"></script>
        <script src="~/output/js/theme-toggle-button-handler.js"></script>
        @await RenderSectionAsync("Scripts", required: false)
    </head>

    <body>
        <partial name="~/Views/Shared/Layouts/NavigationBar/_TopBar.cshtml"/>
        <div id="root-layout">
            <partial name="~/Views/Shared/Layouts/NavigationBar/_NavigationBar.cshtml"/>
            @RenderBody()
        </div>
    </body>
</html>