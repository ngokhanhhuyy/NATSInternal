@page "/"
@page "/Home"
@rendermode InteractiveServer

<PageTitle>Home</PageTitle>

<h1>Hello, world!</h1>

<button class="btn btn-primary" @onclick="IncrementCount">
    @Count
</button>

Welcome to your new app.
@code {
    private int Count { get; set; }

    private void IncrementCount()
    {
        Count++;
    }
}